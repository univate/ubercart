/**
 * @file
 * Install file for uc_2checkout.module.
 */

/**
 * Implementation of hook_uninstall().
 */
function uc_2checkout_uninstall() {
  db_delete('variable')
    ->condition('name', 'uc_2checkout_%', 'LIKE')
    ->execute();
}

