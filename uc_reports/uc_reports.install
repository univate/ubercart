/**
 * @file
 * Install hooks for uc_reports.module.
 */

/**
 * Implementation of hook_uninstall().
 */
function uc_reports_uninstall() {
  db_delete('variable')
    ->condition('name', 'uc_reports_%', 'LIKE')
    ->execute();
}

